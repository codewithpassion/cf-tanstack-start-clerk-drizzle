{
	"name": "cf-tanstack-start",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"serve": "vite preview",
		"test": "vitest run",
		"deploy": "bun run build && wrangler deploy",
		"preview": "bun run build && vite preview",
		"cf-typegen": "wrangler types workers/worker-configuration.d.ts --env-interface CloudflareBindings",
		"check:tsc": "bunx tsc --noEmit -p ./tsconfig.json",
		"biome:check": "biome check --fix && biome format --write",
		"check": "bun cf-typegen && bun run build && bun run biome:check && bun run check:tsc",
		"tail:prod": "wrangler tail -e production",
		"tail:staging": "wrangler tail -e staging",
		"db:apply": "wrangler d1 migrations apply DB --local",
		"db:deploy:prod": "drizzle-kit generate && wrangler d1 migrations apply app-db --remote --env production",
		"db:deploy:staging": "drizzle-kit generate && wrangler d1 migrations apply app-db --remote --env staging",
		"db:deploy:staging-bo": "drizzle-kit generate && wrangler d1 migrations apply app-db --remote --env staging-bo",
		"db:gen": "drizzle-kit generate",
		"db:update": "drizzle-kit generate && wrangler d1 migrations apply app-db --local",
		"prepare": "husky"
	},
